{
    "name": "laravelers-academy/zoom-meeting",
    "description": "Integración de la API de Zoom para la administración de reuniones de mi cuenta de Zoom",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Homero Raul",
            "email": "h.raulvc@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "LaravelersAcademy\\ZoomMeeting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelersAcademy\\ZoomMeeting\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7",
        "phpunit/phpunit": "^9",
        "guzzlehttp/guzzle": "^7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelersAcademy\\ZoomMeeting\\Providers\\ZoomMeetingServiceProvider",
                "LaravelersAcademy\\ZoomMeeting\\Providers\\ZoomRouteServiceProvider"
            ]
        }
    }
}
